---
const buttons = [{ title: "Meeting Notes", href: "/posts" }];
---

<header
  class="bg-brand-100 text-2xl px-4 flex justify-between items-center bg-opacity-30"
>
  <a href="/" class="font-serif">
    <img class="w-56" src="/images/rwpoa-logo-sm.png" alt="site logo" />
  </a>
  <div>
    {
      buttons.map(btn => (
        <button>
          <a href={btn.href}>{btn.title}</a>
        </button>
      ))
    }
  </div>
</header>
